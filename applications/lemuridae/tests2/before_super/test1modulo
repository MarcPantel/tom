term modulo {
}

prop modulo {
  r : [n] P(s(n)) -> P(n)
}

prop fold {
}


rootR(
  <r:P(z())=>P(s(s(z())))> cut(
    <a7:P(z())=>P(s(s(z())))> implyR(
      <x2:P(z())><a4:P(s(s(z())))> cut(
        <a1:P(s(z()))> ax(x2,a1),
        <x4:P(s(z()))> cut(
          <a9:P(s(z()))> ax(x4,a9),
          <x5:P(s(s(z())))> ax(x5,a4))),a7),
    <x6:P(z())=>P(s(s(z())))> ax(x6,r))
  )
)
