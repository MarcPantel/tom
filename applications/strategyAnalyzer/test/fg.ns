abstract syntax
T = a()
  | b()
  | g(T)
  | f(T)

strategies

r1() =  [ g(f(x)) -> f(g(x)) ]

obu(s) = mu x.(one(x) <+ s)
repeat(s) = mu x.((s;x)<+identity)
innermost(s) = repeat(obu(s))
innermost2(s) = mu x.(all(x);((s;x)<+identity))
propagate(s) = mu x. (s ; (all(x)<+identity))
bup(s) = mu y.(all(y);(propagate(s)<+identity))

mainStrat() = innermost(r1())
