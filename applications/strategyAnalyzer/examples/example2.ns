
abstract syntax
T = a()
  | b()
  | i(G)
  | f(T,T)

G = g(T)

strategies

r1() = [ a() -> f(b(),b()) ]

r2() = [ b() -> b() ]

r3() = [ b() -> a() ]

r4() = [ f(x,x) -> x ]

innermost(s) = mu x.(all(x);((s;x)<+identity)) 

td(s) = mu y.((s<+identity);all(y)) 

repeat(s) = mu x.((s<+identity);x)

mainStrat() = innermost(r3())
