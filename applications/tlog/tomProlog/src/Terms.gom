
	
		module PrologTerms
		imports int String
		abstract syntax
		
		Term = constant(name:String) | variable(name:String,isTemp:int) | function(name:String,terms:TermList)
		TermList = tList(Term*)
		
		Fact = fact(name:String,arity:int,subTerms:TermList)
		FactList = fList(Fact*)

		Rule = rule(goal:Fact,body:FactList)
		RuleList = rList(Rule*)
		
		Substitution = subs(variable:Term,constant:Term)
		SubstitutionList = sList(Substitution*)
		
