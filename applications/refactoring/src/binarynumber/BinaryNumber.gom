module binarynumber.BinaryNumber
imports String 
abstract syntax

BinaryNumber = IntegralNumber(IntegralPart:BitList)
             | RationalNumber(IntegralPart:BitList,FractionalPart:BitList)

BitList = SingularBitList(Bit:Bit)
        | PluralBitList(BitList:BitList,Bit:Bit)

Bit = Zero()
    | OneB()
