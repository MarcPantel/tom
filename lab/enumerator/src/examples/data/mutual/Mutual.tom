
%include { int.tom }

%typeterm B {
  implement { examples.data.mutual.types.B }
  is_sort(t) { ($t instanceof examples.data.mutual.types.B) }

  equals(t1,t2) { ($t1==$t2) }

}

%typeterm A {
  implement { examples.data.mutual.types.A }
  is_sort(t) { ($t instanceof examples.data.mutual.types.A) }

  equals(t1,t2) { ($t1==$t2) }

}
%op B b() {
  is_fsym(t) { ($t instanceof examples.data.mutual.types.b.b) }
  make() { examples.data.mutual.types.b.b.make() }
}

%op B grr(a:A) {
  is_fsym(t) { ($t instanceof examples.data.mutual.types.b.grr) }
  get_slot(a, t) { $t.geta() }
  make(t0) { examples.data.mutual.types.b.grr.make($t0) }
}

%op A a() {
  is_fsym(t) { ($t instanceof examples.data.mutual.types.a.a) }
  make() { examples.data.mutual.types.a.a.make() }
}

%op A foo(b:B) {
  is_fsym(t) { ($t instanceof examples.data.mutual.types.a.foo) }
  get_slot(b, t) { $t.getb() }
  make(t0) { examples.data.mutual.types.a.foo.make($t0) }
}

%op A hoo(a:A, v:int) {
  is_fsym(t) { ($t instanceof examples.data.mutual.types.a.hoo) }
  get_slot(a, t) { $t.geta() }
  get_slot(v, t) { $t.getv() }
  make(t0, t1) { examples.data.mutual.types.a.hoo.make($t0, $t1) }
}

