module JavaHook

public
sorts Term
abstract syntax
Empty() -> Term
Empty:make() {
  System.out.println("Empty()");
}
Unary(slot:Term) -> Term
Binary(ls:Term,rs:Term) -> Term
Binary:make(l,r) {
  System.out.println("Binary("+l+", "+r+")");
}
Ternary(ls:Term,cs:Term,rs:Term) -> Term
Unary:make(a) {
  System.out.println("Unary("+a+")");
}
Vary(Term*) -> Term
Vary:make_insert(e,l) {
  System.out.println("Vary("+e+", "+l+")");
}
Ternary:make(t1,t2,t3) {
  System.out.println("Ternary("+t1+", "+t2+", "+t3+")");
}
