module Builtin
imports int boolean String ATerm ATermList long char float double

abstract syntax
Wrapper = | Int(i:int)
          | IntBis(i:int)
          | Name(n:String)
          | NameBis(n:String)
          | Node(aterm:ATerm)
          | NodeBis(aterm:ATerm)
          | Long(l:long)
          | LongBis(l:long)
          | Char(c:char)
          | CharBis(c:char)
          | Bool(b:boolean)
          | BoolBis(b:boolean)
          | concWrap(Wrapper*)
          | concInt(int*)
          | concLong(long*)
          | concBool(boolean*)
          | concChar(char*)
          | Real(r:float)
          | RealBis(r:float)
          | Double(d:double)
          | DoubleBis(d:double)

