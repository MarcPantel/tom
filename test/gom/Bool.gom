module Bool
abstract syntax
Bool = True()
     | False()
     | Not(b:Bool)
     | And(l:Bool,r:Bool)
     | Or(l:Bool,r:Bool)
Not:make(b) {
  %match(Bool b) {
    And(l,r) -> { return `Or(Not(l),Not(r)); }
    Or(l,r)  -> { return `And(Not(l),Not(r)); }
    True()   -> { return `False(); }
    False()  -> { return `True(); }
  }
}
