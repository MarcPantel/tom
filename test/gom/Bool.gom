module Bool
public
sorts Bool
abstract syntax
True() -> Bool
False() -> Bool
Not(b:Bool) -> Bool
Not:make_before(b) {
  %match(Bool b) {
    And(l,r) -> { return `Or(Not(l),Not(r)); }
    Or(l,r)  -> { return `And(Not(l),Not(r)); }
    True()   -> { return `False(); }
    False()  -> { return `True(); }
  }
}
And(l:Bool,r:Bool) -> Bool
Or(l:Bool,r:Bool) -> Bool
