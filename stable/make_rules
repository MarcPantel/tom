TSADT_STABLE=$(top_srcdir)/stable/adt/TomSignature-${VERSION}.jar
SETADT_STABLE=$(top_srcdir)/stable/adt/Set-${VERSION}.jar
TNODEADT_STABLE=$(top_srcdir)/stable/adt/TNode-${VERSION}.jar
CONCURRENT_JAR  =$(top_srcdir)/lib/concurrent.jar:${prefix}/share/concurrent.jar

CLASSPATH_ENV   = CLASSPATH=$(JAVAROOT):$(TSADT_STABLE):$(SETADT_STABLE):$(TNODEADT_STABLE):$(srcdir)/$(JAVAROOT):$(JAVA_ATERM):$(JAVA_APIGEN):$(SHARED_OBJECTS):${CONCURRENT_JAR}:$$CLASSPATH

JAVAROOT        = ${top_builddir}/stable/

#include $(top_srcdir)/stable/make_rules
