  is_fsym(t) { (t!= null) && t.isNotationNode()}
  get_slot(publicId,t) { t.getPublicId()}
  get_slot(systemId,t) { t.getSystemId()}
  make(t0, t1) { getTNodeFactory().makeTNode_NotationNode(t0, t1)}
}

