<?xml version="1.0" encoding="UTF-8"?>
<project name="Build the ravage bench" default="build" basedir="../..">
  <import file="../../common.xml"/>
  <target name="clean" description="Cleans bench directory">
    <delete dir="${bench.gen}/compilation/ravage"/>
    <delete dir="${bench.build}/compilation/ravage"/>
  </target>
  <target name="build" depends="init">
  	<gomcompile example="compilation/ravage"/>
  </target>
</project>
