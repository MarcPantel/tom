<?xml version="1.0" encoding="UTF-8"?>
<project name="Build the moleculargraphs bench" default="build" basedir="../..">
  <import file="../../common.xml"/>
  <target name="clean" description="Cleans bench directory">
    <delete dir="${bench.gen}/compilation/moleculargraphs"/>
    <delete dir="${bench.build}/compilation/moleculargraphs"/>
  </target>
  <target name="build" depends="init">
  	<gomcompile example="compilation/moleculargraphs"/>
    <tomcompile example="compilation/moleculargraphs"/>
    <javacompile example="compilation/moleculargraphs"/>
  </target>
</project>
