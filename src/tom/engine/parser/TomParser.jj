XMLTerm XMLPlainTerm(TomName astAnnotedName) throws TomException: /* in TOM mode */
{
  ArrayList list = new ArrayList();
  Token name, slotName;
  XMLTerm term;
  Option option = null;
  Option annotedName = (astAnnotedName==null)?null:tsf().makeOption_TomNameToOption(astAnnotedName);
  ArrayList optionList = new ArrayList();
}
{
  // elem(...) or elem[an_integer](...)
  LOOKAHEAD(2)
    name = <TOM_IDENTIFIER>
      {
	text += name.image;
	optionList.add(ast().makeOriginTracking(name.image,getLine(), currentFile));
      }
    [ 
     <TOM_LBRACKET> slotName = <TOM_IDENTIFIER> <TOM_RBRACKET>
      {
	text += "["+slotName.image+"]";
	optionList.add(tsf().makeOption_XMLPosition(slotName.image));
      }
    ]
     <TOM_LPAREN> 
      {text += "(";}
      [
       term = XMLTerm()               
	{ list.add(term); }  
       ( <TOM_COMMA> {text += " , ";} term = XMLTerm() { list.add(term); } )*
      ]
     <TOM_RPAREN>
      {
        text += ")";
      }
    {
      if(annotedName!=null) { optionList.add(annotedName); }
      option = ast().makeOption(ast().makeOptionList(optionList));
      return tsf().makeXMLTerm_Element(
	      tsf().makeTomName_Name(name.image),
	      ast().makeList(list), 
	      option);
    }
  | // var
     term = XMLVariable(astAnnotedName)
      {	
	return term;
      }
  | // _
    <TOM_UNDERSCORE>
    {
      text += "_";
      optionList.add(ast().makeOriginTracking("_",getLine(), currentFile));
      if(annotedName!=null) { optionList.add(annotedName); }
      option = ast().makeOption(ast().makeOptionList(optionList));
      return tsf().makeXMLTerm_XMLPlaceholder(option);
    }
  | // &attribute(data) | &attribute
    <TOM_AND> {text += "&";}
    term = XMLAttribute(astAnnotedName)
    
    {
      return term;
    }
    
  | // #NODE | #ATTRIBUTE | #TEXT | #COMMENT | #PROCESSING-INSTRUCTION
    <TOM_SHARP> name = <TOM_IDENTIFIER> 
     [
     <TOM_LPAREN> 
         {text += "(";}    
       term = XMLVariable(astAnnotedName)               
	{ list.add(term); }
       ( <TOM_COMMA> {text += " , ";} term = XMLVariable(astAnnotedName) { list.add(term); } )*
     <TOM_RPAREN>
     ]
     {
       text += "#"+name.image;
       String rWord = name.image.toUpperCase();
       optionList.add(ast().makeOriginTracking(name.image,getLine(), currentFile));
       if(annotedName!=null) { optionList.add(annotedName); }
       option = ast().makeOption(ast().makeOptionList(optionList));
       return tsf().makeXMLTerm_ReservedWord(tsf().makeTomName_Name(rWord),
					     ast().makeList(list),
					     option);
     }
}

