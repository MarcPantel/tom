      }

      switchToDefaultMode(); /* switch to DEFAULT mode */
        //term = ast().buildBackQuoteTerm(tokenList);
      term = tomBackQuoteParser.buildBackQuoteTerm(tokenList);
      list.add(term);
    }
}


