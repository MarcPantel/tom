void BackQuoteTerm(ArrayList list) throws TomException: /* in DEFAULT mode */
{
  TomTerm term;
  Option orgTrack;

  Token tk;
  int parenLevel = 0;
  boolean backQuoteMode = false;
  ArrayList tokenList = new ArrayList();
}
{
  <BACKQUOTE_TERM> /* switch to TOM mode */
    {
      addPreviousCode(list);
      orgTrack = ast().makeOriginTracking("Backquote",getLine(), currentFile);
      backQuoteMode = true;
      while(backQuoteMode) {
        tk = getNextToken();
        tokenList.add(tk);
        if(tk.kind == TOM_LPAREN) {
          parenLevel++;
        } else if(tk.kind == TOM_RPAREN) {
          parenLevel--;
          if(parenLevel==0) {
            backQuoteMode = false;
          }
