  term = UnamedVariableStarOrVariableStar(optionList) { return term; }
  |
  ( // name = [anno2@](String|Identifier)
    LOOKAHEAD(2)
    id = <TOM_IDENTIFIER> <TOM_EQUAL> {text+=id.image+"=";}
    [
      LOOKAHEAD(2)
    anno2 = <TOM_IDENTIFIER> <TOM_AT>
