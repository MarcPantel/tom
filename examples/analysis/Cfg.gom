module Cfg
imports Ast int
abstract syntax

Cfg = BeginIf(ast:Ast,refSuccess:Reference,refFailure:Reference)
    | Affect(ast:Ast,ref:Reference)
    | Free(var:Variable,ref:Reference)
    | BeginWhile(ast:Ast,ref:Reference)
    | FailWhile(ref:Reference)
    | Nil(ref:Reference)
    | End()
    | ConcCfg(Cfg*)
    | LabCfg(label:String,cfg:Cfg)

Reference = Pos(int*)
          | Label(label:String)

Pos:interface{
tom.library.strategy.mutraveler.MuReference
  }
